// Online C compiler to run C program online
#include <stdio.h>
#include <complex.h>
#include<stdlib.h>

double SQRT(int N,int Tol){
    
    double x=N;
    
    double root;
    
    int count=0;
    
    while(1){
        count++;
        root = 0.5*(x+(N/x));
        
        if (abs(root-x)<Tol){
            break;
        }
        
        x=root;
        
    }
    
    
}


