#include <stdio.h>
#include <complex.h>
#include<stdlib.h>

double SQRT(int N,int Tol){
    
    double x=N;
    
    double root;
    
    int count=0;
    
    
    while(1){
        count++;
        root = 0.5*(x+(N/x));
        
        if (abs(root-x)<Tol){
            break;
        }
        
        x=root;
        
    }
    
    
}

double MMULN(int M[2][2],double B[2]){
  
  double EV[2];
  int i,j,ni;
  double N;
  
  for (i=0;i<2;i++){
    for (j=0;j<2;j++){
      EV[i]=M[i][j]*B[j]+M[i][j]*B[j];
    }
  }
  
  for (ni=0;ni<2;ni++){
    N=EV[ni]*EV[ni]
    
  }
 
  
 
  
  
  

  
}





